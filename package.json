{
  "name": "unique_slide_extractor",
  "version": "1.0.0",
  "description": "A CLI app to extract unique slide thumbnails from webinar videos.",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "ts-node src/main.ts",
    "extract": "ts-node src/extractScreenshots.ts",
    "crop": "ts-node src/cropper.ts",
    "removeSimilar": "ts-node src/removeSimilar.ts",
    "tsc": "tsc --build tsconfig.json",
    "watch": "ts-node --esm src/daemon.ts"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "child_process": "^1.0.2",
    "chokidar": "^4.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "path": "^0.12.7",
    "sharp": "^0.33.5",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^18.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^4.8.0"
  }
}
